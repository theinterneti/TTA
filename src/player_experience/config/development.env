# Development Environment Configuration
ENVIRONMENT=development
API_DEBUG=true
API_RELOAD=true
API_LOG_LEVEL=DEBUG

# Server settings
API_HOST=0.0.0.0
API_PORT=8080

# Security settings (development only)
API_JWT_SECRET_KEY=dev-secret-key-not-for-production
API_ACCESS_TOKEN_EXPIRE_MINUTES=60

# Database settings
API_REDIS_URL=redis://localhost:6379
API_NEO4J_URL=bolt://localhost:7687
API_NEO4J_USERNAME=neo4j
API_NEO4J_PASSWORD=password

# CORS settings
API_CORS_ORIGINS=http://localhost:3000,http://localhost:8080,https://localhost:3000,https://localhost:8080

# Rate limiting (relaxed for development)
API_RATE_LIMIT_CALLS=1000
API_RATE_LIMIT_PERIOD=60

# Therapeutic settings
API_CRISIS_DETECTION_ENABLED=true
API_CRISIS_HOTLINE=988