{
  "name": "tta-storybook-env",
  "version": "1.0.0",
  "description": "Isolated Storybook environment with Vite 6 compatibility",
  "private": true,
  "dependencies": {
    "vite": "^6.0.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
